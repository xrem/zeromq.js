{
  "name": "zeromq",
  "version": "5.1.0",
  "description": "ZeroMQ for node.js",
  "main": "index",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zeromq/zeromq.js.git"
  },
  "dependencies": {
    "nan": "^2.10.0",
    "prebuild-install": "5.3.2"
  },
  "devDependencies": {
    "electron-mocha": "^8.1.2",
    "jsdoc": "^3.5.4",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prebuild": "^9.1.0",
    "semver": "^6.3.0",
    "should": "^13.0.0"
  },
  "engines": {
    "node": ">=6.0"
  },
  "binary": {
    "host": "https://github.com",
    "remote_path": "xrem/zeromq.js/raw/master/prebuilds"
  },
  "scripts": {
    "build:libzmq": "node scripts/preinstall.js",
    "install": "node scripts/prebuild-install.js || (node scripts/preinstall.js --force && node-gyp rebuild)",
    "prebuild": "prebuild --all --strip",
    "build:docs": "jsdoc -R README.md -d docs lib/*.js",
    "test": "mocha --expose-gc --slow 300",
    "test:electron": "electron-mocha --slow 300",
    "precoverage": "nyc npm run test",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "keywords": [
    "zeromq",
    "zmq",
    "0mq",
    "Ã¸mq",
    "libzmq",
    "native",
    "binding",
    "addon"
  ],
  "license": "MIT",
  "author": "Justin Tulloss <justin.tulloss@gmail.com> (http://justin.harmonize.fm)"
}
